    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "lint-staged": "9.4.1",
    "prettier": "1.19.1"
  },
  "dependencies": {
    "@statechannels/devtools": "0.1.21",
    "easy-table": "1.1.1",
